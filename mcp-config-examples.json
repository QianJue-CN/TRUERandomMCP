{
  "// 方式1: 使用npx运行（推荐，无需安装）": "",
  "mcpServers_npx": {
    "random-org": {
      "command": "npx",
      "args": ["random-org-mcp-server"],
      "env": {
        "RANDOM_ORG_API_KEY": "your_api_key_here"
      }
    }
  },

  "// 方式2: 全局安装后使用命令": "",
  "mcpServers_global": {
    "random-org": {
      "command": "random-org-mcp",
      "env": {
        "RANDOM_ORG_API_KEY": "your_api_key_here"
      }
    }
  },

  "// 方式3: 本地安装后使用node_modules": "",
  "mcpServers_local": {
    "random-org": {
      "command": "node",
      "args": ["node_modules/random-org-mcp-server/dist/index.js"],
      "env": {
        "RANDOM_ORG_API_KEY": "your_api_key_here"
      }
    }
  },

  "// 方式4: 完整配置示例（包含所有可选参数）": "",
  "mcpServers_full": {
    "random-org": {
      "command": "npx",
      "args": ["random-org-mcp-server"],
      "env": {
        "RANDOM_ORG_API_KEY": "your_api_key_here",
        "RATE_LIMIT_REQUESTS_PER_SECOND": "1",
        "RATE_LIMIT_BURST_SIZE": "5",
        "REQUEST_TIMEOUT_MS": "10000",
        "MAX_RETRIES": "3",
        "RETRY_DELAY_MS": "1000"
      }
    }
  },

  "// Claude Desktop 完整配置示例": "",
  "claude_desktop_config": {
    "mcpServers": {
      "random-org": {
        "command": "npx",
        "args": ["random-org-mcp-server"],
        "env": {
          "RANDOM_ORG_API_KEY": "your_api_key_here"
        }
      }
    }
  }
}
